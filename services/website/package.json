{
  "name": "@your-analytics/website",
  "description": "The website for www.your-analytics.org",
  "version": "0.0.1",
  "scripts": {
    "build": "run-s clean && sapper build --legacy",
    "clean": "rm -fr __sapper__",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "run-s clean && sapper dev",
    "export": "run-s clean && NODE_ENV=production sapper export --legacy --entry '/ /auth /dashboard'",
    "start": "node __sapper__/build",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "chartjs-chart-geo": "^2.1.0",
    "compression": "^1.7.1",
    "date-fns": "^2.15.0",
    "magic-sdk": "^2.3.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@tailwindcss/ui": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.0.8",
    "tailwindcss": "^1.5.2"
  }
}
